<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent" >        <ViewSwitcher        android:id="@+id/check_now_switcher"        android:layout_width="wrap_content"        android:layout_height="match_parent" >                <ListView            android:id="@+id/check_now_list"            android:layout_width="fill_parent"            android:layout_height="fill_parent"            android:gravity="center"            android:cacheColorHint="@android:color/transparent"        	android:divider="@drawable/list_divider_line"        	android:dividerHeight="1dip"  >        </ListView>                <include layout="@layout/layout_progress_page" />            </ViewSwitcher>        <TextView        android:id="@+id/check_now_tv"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerInParent="true"        android:text="@string/check_now"        android:textSize="20sp" />    </RelativeLayout>